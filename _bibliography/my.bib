issn = {15749541},
journal = {Ecological Informatics},
keywords = {Camera traps,Data management and analyses,Free and open source software,Geographical information systems,Wildlife monitoring},
pages = {6--11},
publisher = {Elsevier B.V.},
title = {{Advances in camera trap data management tools: Towards collaborative development and integration with GIS}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S157495411500117X},
volume = {30},
year = {2015}
}
